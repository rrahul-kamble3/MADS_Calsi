<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.madscalsi">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/app_calculator"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_calculator"
        android:supportsRtl="true"
        android:theme="@style/Theme.MADSCalsi">
        <activity android:name=".Activity.HistoryActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"></activity>
        <activity android:name=".Activity.MainActivity" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>